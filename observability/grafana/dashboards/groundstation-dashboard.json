{
  "title": "Groundstation API â€” SRE Overview",
  "tags": ["space-sre-lab", "groundstation-api", "sre"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Rate (RPS)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"groundstation-api\"}[5m]))",
          "legendFormat": "rps"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5xx)",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"groundstation-api\",status=~\"5..\"}[5m]))",
          "legendFormat": "5xx/s"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (seconds)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"groundstation-api\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    }
  ]
}
